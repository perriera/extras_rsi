    "version": "0.2.0",
    "configurations": [
        {
            "name": "run-unittests",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/run-unittests-rsi",
            "args": [
                "[SemaphoreInterface]"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "Build C++ project",
            "miDebuggerPath": "/usr/bin/gdb"
        },
        {
            "name": "socketpool_client",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/socketpool_client",
            "args": [
                "137.184.218.130",
                "8080",
                "data/exparx.webflow.zip",
                "upload",
                "vendor",
                "download",
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
